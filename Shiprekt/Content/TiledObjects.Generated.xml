<?xml version="1.0" encoding="utf-8"?>
<objecttypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <objecttype name="Bird">
    <property name="MaxFlightSpeed" type="float" default="30" />
    <property name="MaxTurnRate" type="float" default="1.57" />
    <property name="MaxSecondsToFlightStateChange" type="float" default="3" />
    <property name="MinFlightSpeed" type="float" default="8" />
    <property name="MinBirdAltitude" type="float" default="20" />
    <property name="MaxBirdAltitude" type="float" default="200" />
  </objecttype>
  <objecttype name="Bullet">
    <property name="BulletSpeed" type="float" default="300" />
    <property name="ReloadTimeInSeconds" type="float" default="3" />
    <property name="BulletDistance" type="float" default="200" />
    <property name="DamageToDeal" type="int" default="20" />
  </objecttype>
  <objecttype name="Cloud">
    <property name="CloudAltitudeMin" type="float" default="100" />
    <property name="CloudAltitudeMax" type="float" default="200" />
  </objecttype>
  <objecttype name="ShipDeathEffect">
    <property name="EffectDurationSeconds" type="float" default="4" />
  </objecttype>
  <objecttype name="Ship">
    <property name="CarData" type="Shiprekt.DataTypes.RacingEntityValues" default="DefaultValues in RacingEntityValues.csv" />
    <property name="ShipEntityValuesInstance" type="Shiprekt.DataTypes.ShipEntityValues" default="DefaultValues in ShipEntityValues.csv" />
    <property name="Dead" type="bool" default="false" />
    <property name="Health" type="int" />
    <property name="MaxSailAccelerationBoost" type="float" default="20" />
    <property name="MinSpeed" type="float" default="10" />
    <property name="HeavyRamAngle" type="float" default="10" />
    <property name="MediumRamAngle" type="float" default="20" />
    <property name="GlancingRamAngle" type="float" default="30" />
    <property name="HeavyRamDamage" type="int" default="20" />
    <property name="MediumRamDamage" type="int" default="10" />
    <property name="GlancingRamDamage" type="int" default="5" />
    <property name="RamCooldown" type="float" default="3" />
    <property name="MinimumRamSpeed" type="float" default="50" />
    <property name="SecondsBetweenShotsMin" type="float" default="1.5" />
  </objecttype>
  <objecttype name="Wreckage" />
</objecttypes>